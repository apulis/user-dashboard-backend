{
  "name": "hello-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=develop CONFIG_PATH='' nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=prod CONFIG_PATH=/etc/app/config/local.config node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@casbin/etcd-watcher": "^1.0.0",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^6.10.14",
    "@nestjs/config": "^0.4.0",
    "@nestjs/core": "^6.10.14",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^6.10.14",
    "@nestjs/swagger": "^4.5.1",
    "@nestjs/typeorm": "^6.3.3",
    "@types/cache-manager": "^2.10.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express-rate-limit": "^5.0.0",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/lodash": "^4.14.165",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "axios": "^0.19.2",
    "cache-manager": "^3.3.0",
    "cache-manager-redis-store": "^2.0.0",
    "casbin": "^4.2.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "express-rate-limit": "^5.1.3",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "mysql": "2.18.1",
    "mysql2": "^2.1.0",
    "nest-schedule": "^0.6.4",
    "nestjs-rate-limiter": "^1.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pm2": "4.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "stacktrace-js": "^2.0.2",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24",
    "typeorm-adapter": "^1.0.4",
    "wechat-oauth": "^1.5.0",
    "wechat-oauth-ts": "^1.9.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.10.14",
    "@types/dotenv": "^8.2.0",
    "@types/hapi__joi": "^16.0.12",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
